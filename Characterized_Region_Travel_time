{"cells":[{"cell_type":"code","execution_count":1,"metadata":{"id":"WWFzlUeq_hcZ","executionInfo":{"status":"ok","timestamp":1736175491262,"user_tz":300,"elapsed":147,"user":{"displayName":"Hyemi Kim","userId":"01114510086554383980"}}},"outputs":[],"source":["import numpy as np\n","import matplotlib.pyplot as plt"]},{"cell_type":"markdown","source":["# Linear Demand"],"metadata":{"id":"mbY-dVbpQ8dB"}},{"cell_type":"code","source":["# x: b2/b1, y: Lambda2/Lambda1\n","\n","# Define the inequality function\n","def draw_lienar(r, legend=True):\n","  # y=b2/b1, x=\\Lambda2/\\Lambda1\n","  def inequality1(x, y): # p1(alpha) = 0 for 0 < alpha < alpha^*\n","    return np.sqrt(y) - np.sqrt(1/y) > 2*np.sqrt(1/x) + r * np.sqrt(1/y)\n","\n","  def inequality2(x, y): # p2^* >= p1^*\n","    return r * (y+1/y-2) + 2 * (y-1) * (np.sqrt(x) + np.sqrt(y)) * (1/x*np.sqrt(1/y)-1/y*np.sqrt(1/x)) > (y+1) * (np.sqrt(y) - np.sqrt(x)) * (1/x*np.sqrt(1/y)-1/y*np.sqrt(1/x))\n","\n","  def inequality3(x, y): # W'(0) >= 0\n","    return y > x\n","\n","  num = 1000\n","  k = 8\n","\n","  # Create a grid of points using meshgrid\n","  x_vals = np.linspace(1e-10, k, num)\n","  y_vals = np.linspace(1, k, num)\n","  X, Y = np.meshgrid(x_vals, y_vals)\n","\n","  results1 = inequality1(X, Y)\n","\n","  results2 = ~(results1) & inequality2(X, Y) & inequality3(X,Y)\n","\n","  results3 = ~(results1) & ~inequality3(X,Y)\n","\n","  results4 = ~inequality2(X, Y) & inequality3(X,Y)\n","\n","  fig, ax = plt.subplots()\n","  fig.set_size_inches(6, 6)\n","\n","  # Plot the points that satisfy each inequality with different colors\n","  marker = '.'\n","  colors = [plt.cm.viridis((4-i) / 4.0) for i in range(4)]\n","  colors = ['yellowgreen', 'skyblue', 'orange', 'tomato']\n","\n","  plt.scatter(X[results1], Y[results1], color = colors[0], marker=marker, label='$(+,+,+)$', s=0.1) # green\n","  plt.scatter(X[results2], Y[results2], color = colors[1], marker=marker, label='$(+,-,+)$', s=0.1) # skyblue\n","  plt.scatter(X[results3], Y[results3], color = colors[2], marker=marker, label='$(-,-,+)$', s=0.1) # orange\n","  plt.scatter(X[results4], Y[results4], color = colors[3], marker=marker, label='$(-,+,-)$', s=0.1) # tomato\n","\n","  ax.set(ylabel=r'$E[V_2]/E[V_1]$')\n","  ax.set(xlabel=r'$\\Lambda_2/\\Lambda_1$')\n","  ax.xaxis.label.set_fontsize(28)\n","  ax.yaxis.label.set_fontsize(28)\n","\n","  ax.plot([1 for i in range(num+1)], [k*i/num for i in range(num+1)], color='black', linestyle='--')\n","\n","  # Add a legend\n","  if legend:\n","    plt.legend(title=r'$\\left(\\Delta\\mathcal{W}, \\Delta\\mathcal{S}_1, \\Delta\\mathcal{S}_2\\right)$', loc='upper right', markerscale=50)\n","\n","  plt.xlim([0, k])\n","  plt.ylim([1, k])\n","\n","  plt.show()"],"metadata":{"id":"picRqic47fdP","executionInfo":{"status":"ok","timestamp":1736175491491,"user_tz":300,"elapsed":230,"user":{"displayName":"Hyemi Kim","userId":"01114510086554383980"}}},"execution_count":2,"outputs":[]},{"cell_type":"code","source":["draw_lienar(r=0, legend=False) # travel rate = infinite (no travel time)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":573},"id":"_5j406IfsnRn","executionInfo":{"status":"ok","timestamp":1736175493299,"user_tz":300,"elapsed":1810,"user":{"displayName":"Hyemi Kim","userId":"01114510086554383980"}},"outputId":"764a9c31-f065-4271-bb20-bed5a3bc8fed"},"execution_count":3,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 600x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["draw_lienar(r=1, legend=False) # travel rate = market size of 1"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":573},"id":"3ObcaKUvQPzZ","executionInfo":{"status":"ok","timestamp":1736175494787,"user_tz":300,"elapsed":1493,"user":{"displayName":"Hyemi Kim","userId":"01114510086554383980"}},"outputId":"9e8873f3-ebbc-444b-89b9-d4958bcc95f8"},"execution_count":4,"outputs":[{"output_type":"display_data","data":{"te